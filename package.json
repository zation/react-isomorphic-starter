    "webpack-hot-middleware": "^2.18.0",
    "webpack-node-externals": "^1.6.0"
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,less,scss,sss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "lint:css": "stylelint \"src/**/*.{css,less,scss,sss}\"",
    "lint:staged": "lint-staged",
    "lint": "yarn run lint:js && yarn run lint:css",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "NODE_ENV=production babel-node tools/run build",
    "build:stats": "NODE_ENV=production yarn run build -- --analyse",
    "render": "babel-node tools/run render",
    "serve": "babel-node tools/run run-server",
    "start": "babel-node tools/run dev-server",
    "mocker": "nodemon --watch ./mocker ./mocker/index.js --exec babel-node"
  },
  "pre-commit": []
}
